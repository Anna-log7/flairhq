{
  "name": "FlairHQ",
  "version": "3.0.0",
  "description": "A project to allow easy adding of flair applications for subreddits (focusing initially on /r/pokemontrades) and easy moderation for moderators.",
  "scripts": {
    "start": "npm-run-all --parallel open:client dev",
    "start:debug": "npm-run-all --parallel open:client debug",
    "open:client": "webpack-dev-server --open",
    "build": "npm run build:prod",
    "build:dev": "webpack",
    "build:prod": "webpack --mode production",
    "clean": "rimraf .tmp && mkdirp .tmp/public",
    "prod": "sails lift --prod",
    "dev": "sails lift --dev",
    "debug": "node --inspect app.js",
    "prettier": "prettier --write 'assets/**/*.js'"
  },
  "main": "app.js",
  "repository": {
    "type": "git",
    "url": "git://github.com/yamanickill/flairhq.git"
  },
  "author": "YaManicKill",
  "license": "Apache-2.0",
  "keywords": [],
  "dependencies": {
    "async": "~1.4.2",
    "babel-eslint": "^4.1.4",
    "chai": "^3.4.1",
    "connect-mongo": "^0.8.2",
    "crypto": "^0.0.3",
    "ejs": "~2.5.5",
    "include-all": "~0.1.3",
    "jquery-browserify": "^1.8.1",
    "lodash": "^3.10.1",
    "mocha": "^2.3.4",
    "moment": "~2.19.3",
    "ng-mask": "^3.0.12",
    "node-cache": "^3.0.0",
    "node-sha1": "^1.0.1",
    "pako": "^0.2.8",
    "passport": "^0.3.0",
    "passport-reddit": "^0.2.4",
    "q": "1.4.1",
    "rc": "~1.1.2",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "regex": "^0.1.1",
    "request": "~2.64.0",
    "request-promise": "^1.0.2",
    "sails": "~0.11.2",
    "sails-disk": "~0.10.8",
    "sails-hook-schedule": "^0.2.2",
    "sails-hook-winston": "^1.1.0",
    "sails-mongo": "^0.11.4",
    "sails.io.js": "^0.11.7",
    "sha256": "^0.2.0",
    "snudown-js": "^1.4.0",
    "socket.io-browserify": "^0.9.6",
    "socket.io-client": "^1.3.7"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.6.2",
    "@babel/preset-react": "^7.0.0",
    "babel-loader": "^8.0.6",
    "copy-webpack-plugin": "^5.0.4",
    "extract-text-webpack-plugin": "^3.0.2",
    "html-webpack-plugin": "^3.2.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.18.2",
    "rimraf": "^3.0.0",
    "webpack": "^4.41.0",
    "webpack-cli": "^3.3.9",
    "webpack-dev-server": "^3.8.1",
    "webpack-merge": "^4.2.2"
  }
}
